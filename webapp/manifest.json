{
    "_version": "1.63.0",
    "sap.app": {
        "_version": "1.12.0",
        "id": "ui5_walkthroug",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",   
        "dataSources": {
            "personen": {
                "uri": "models/personen.json",
                "type": "JSON"
            }
        }
    },
    "sap.ui": {
       "technology": "UI5",        
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "dependencies": {
            "minUI5Version": "1.108.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "components": {
            "<ui5component1>": {
                "minVersion": "<minComp1Version>"
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ui5_walkthroug.i18n.i18n"
                }

            },
            "personen": { 
                "type": "sap.ui.model.json.JSONModel",
                "uri": "models/personen.json"          
            }
        },
        "rootView": {
            "viewName": "ui5_walkthroug.view.App",
            "type": "XML",
            "id": "app"
        },
        "handleValidation": false,
        "config": {
        },
        "contentDensities": {
            "compact": false,
            "cozy": false
        }
    }
}
