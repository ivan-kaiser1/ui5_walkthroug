<mvc:View
    controllerName="ui5_walkthroug.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
  
  <Table items="{/personen}">
    <columns>  
        <Column>
            <Text text="Vorname"/>
        </Column>
        <Column>
            <Text text="Nachname"/>
        </Column>
        <Column>
            <Text text="Geburtsdatum"/>
        </Column>
    </columns>
    <items>
        <ColumnListItem>
            <cells>
                <Text text="{Vorname}"/>
                <Text text="{Nachname}"/>
                <Text text="{Geburtsdatum}"/>
            </cells>
        </ColumnListItem>
    </items>
    </Table>

</mvc:View>